<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">

  <sitecore>
    <settings>
      
      <!-- Enabled: true/false to determine if publish logging is enabled -->
      <setting name="SitecoreSpark.SPRK.Enabled" value="true" />
      
      <!-- LogFolder: path where publish logs should be written -->
      <setting name="SitecoreSpark.SPRK.LogFolder" value="$(dataFolder)/logs/" />
      
      <!-- LogPrefix: filename prefix for log files; all log files are appended with a dateformat (yyyyMMdd) -->
      <setting name="SitecoreSpark.SPRK.LogPrefix" value="SPRK.log" />
      
    </settings>
  </sitecore>
</configuration>